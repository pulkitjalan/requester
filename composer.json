{
    "name": "pulkitjalan/requester",
    "description": "Requester class to wrap guzzle and retry subscriber",
    "homepage": "https://github.com/pulkitjalan/requester",
    "keywords": ["guzzle", "retry", "http", "curl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Pulkit Jalan"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "guzzlehttp/guzzle": "~5",
        "guzzlehttp/retry-subscriber": "~2",
        "guzzlehttp/log-subscriber": "~1",
        "illuminate/support": "~4"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
            "PulkitJalan\\Requester\\": "src"
        }
    }
}
